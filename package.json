{
  "name": "odds-web-calc",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "wasm:build": "wasm-pack build --target web --debug --out-dir wasm",
    "wasm:release": "wasm-pack build --target web --release --out-dir wasm",
    "wasm:check": "cargo check --lib --target wasm32-unknown-unknown",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier . --write && cargo fmt",
    "lint": "eslint && cargo clippy --target wasm32-unknown-unknown"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.77.2",
    "@unovis/ts": "^1.5.2",
    "@unovis/vue": "^1.5.2",
    "@vueuse/core": "13.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.27.0",
    "flag-icons": "^7.3.2",
    "lucide-vue-next": "0.511.0",
    "postcss": "^8.5.3",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "eslint-plugin-vue": "10.1.0",
    "@eslint/js": "^9.27.0",
    "@types/node": "^22.15.23",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "globals": "16.2.0",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}
