{
  "name": "odds-web-calc",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "wasm:build": "wasm-pack build --target web --debug --out-dir wasm",
    "wasm:release": "wasm-pack build --target web --release --out-dir wasm",
    "wasm:check": "cargo check --lib --target wasm32-unknown-unknown",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier . --write && cargo fmt",
    "lint": "eslint && cargo clippy --target wasm32-unknown-unknown"
  },
  "dependencies": {
    "@internationalized/date": "^3.8.2",
    "@tanstack/vue-query": "5.79.0",
    "@unovis/ts": "^1.5.2",
    "@unovis/vue": "^1.5.2",
    "@vueuse/core": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "flag-icons": "^7.5.0",
    "lucide-vue-next": "0.522.0",
    "pinia": "^3.0.3",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.3.2",
    "tailwind-merge": "3.3.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "24.0.3",
    "@vitejs/plugin-vue": "6.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-vue": "10.1.0",
    "globals": "16.2.0",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "7.0.0",
    "vue-tsc": "^2.2.12"
  }
}
